{
  "name": "hexagonal",
  "version": "1.0.0",
  "main": "index.js",
  "author": {
    "name": "Joanes Nunes Junior Lebarch",
    "email": "joanesdejesusjr@gmail.com"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "npx tsx watch src/index.ts",
    "build": "tsup src --out-dir build",
    "test:watch": "vitest",
    "test:unit": "vitest run src/modules/**/application/__tests__/unit",
    "test:integration": "vitest run src/modules/**/application/__tests__/integration",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prisma:generate": "npx prisma generate --schema ./src/infra/prisma/schema.prisma",
    "prisma:migrate-dev": "npx prisma migrate dev --schema ./src/infra/prisma/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --force --schema ./src/infra/prisma/schema.prisma"
  },
  "keywords": [],
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@vitest/coverage-c8": "0.29.8",
    "pino-pretty": "^13.1.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "4.0.8",
    "vitest": "0.29.8"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.6",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.205.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/instrumentation-amqplib": "^0.52.3",
    "@opentelemetry/instrumentation-express": "^0.54.3",
    "@opentelemetry/instrumentation-grpc": "^0.205.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/instrumentation-pino": "^0.52.3",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@prisma/client": "^6.16.3",
    "@vitest/ui": "0.29.8",
    "amqplib": "^0.10.9",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "pino-loki": "^2.6.0",
    "prisma": "^6.16.3",
    "tsup": "^8.5.0",
    "zod": "^4.1.11"
  }
